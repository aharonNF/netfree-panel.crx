{
	"update_url": "https://clients2.google.com/service/update2/crx",

	"name": "Netfree panel2",
	"version": "0.0.1",
	"author": "zvizvi5@outlook.com",
	"permissions": [
		"http://*/*",
		"https://*/*"
	],
	"browser_action": {
		"default_icon": "img/icon16.png",
		"default_popup": "panel.html"
	},
	"content_scripts": [
	{
		"matches": [
			"http://*/*", "https://*/*"
		],
		"js": [
			"/js/panel.js",
			"/js/feedback-popup.min.js",
			/*"/js/jquery/1.9.1/jquery.min.js",*/
			"/js/sha/sha1.js",
			"/js/async/async.min.js"
		],
	"all_frames": true
	}
	],
	"background": {
		"scripts": [
			"/js/panel.js",
			"/js/feedback-popup.min.js",
			/*"/js/jquery/1.9.1/jquery.min.js",*/
			"/js/sha/sha1.js",
			"/js/async/async.min.js"
		],
		"pages": ["panel.html"]
	},
	"icons": {
		"128": "img/icon128.png",
		"96": "img/icon96.png",
		"72": "img/icon72.png",
		"64": "img/icon64.png",
		"48": "img/icon48.png",
		"24": "img/icon24.png",
		"16": "img/icon16.png"
	},
	"manifest_version": 2
}
